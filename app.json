{
  "expo": {
    "name": "ProductApp",
    "slug": "ProductApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/splachLogo.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "resizeMode": "contain",
        "backgroundColor": "#000"
      },
      "package": "com.musmanali.ProductApp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "expo": {
      "plugins": [
        [
          "expo-location",
          {
            "locationAlwaysAndWhenInUsePermission": "Allow $(ProductApp) to use your location."
          }
        ]
      ]
    },
    "extra": {
      "eas": {
        "projectId": "06b1c2e0-6bd7-4b41-b422-770e154a9289"
      }
    }
  }
}
